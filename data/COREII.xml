<?xml version="1.0" encoding="utf-8"?>
<data name="COREII" lat=_TAG_LAT lon=_TAG_LON maxdepth=_TAG_MAXDEPTH >
    <var name="meteo">
        <longname>Meteorology data</longname>
        <filename>meteo.nc</filename>
        <vartype>meteo</vartype>
        <varname_u10>U_10_MOD</varname_u10>
        <varname_v10>V_10_MOD</varname_v10>
        <varname_slp>SLP</varname_slp>
        <varname_sat>T_10_MOD</varname_sat>
        <varname_hum>Q_10_MOD</varname_hum>
        <varname_cld>CLD</varname_cld>
        <hum_method>4</hum_method>
        <outname>meteo_file.dat</outname>
    </var>
    <var name="precip">
        <longname>Precipitation (mm/day)</longname>
        <filename>gpcp_precip.nc</filename>
        <vartype>scalar</vartype>
        <varname>precip</varname>
        <outname>precip_file.dat</outname>
        <scale_factor>1.1574e-8</scale_factor>
    </var>
</data>

